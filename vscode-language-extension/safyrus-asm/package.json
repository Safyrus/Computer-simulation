{
    "name": "safyrus-asm",
    "displayName": "Safyrus ASM",
    "description": "Syntax highlighting for Safyrus ASM(sasm) language",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "sasm",
            "aliases": ["sASM", "sasm"],
            "extensions": [".sasm"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "sasm",
            "scopeName": "source.sasm",
            "path": "./syntaxes/sasm.tmLanguage.json"
        }]
    }
}